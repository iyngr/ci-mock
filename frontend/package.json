{
  "name": "frontend-workspace",
  "version": "0.1.0",
  "packageManager": "pnpm@9.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "dev:admin": "pnpm --filter admin dev",
    "dev:smartmock": "pnpm --filter smartmock dev",
    "dev:talens": "pnpm --filter talens dev",
    "dev:talens-suite": "pnpm --filter talens-suite dev",
    "build:admin": "pnpm --filter admin build",
    "build:smartmock": "pnpm --filter smartmock build",
    "build:talens": "pnpm --filter talens build",
    "build:talens-suite": "pnpm --filter talens-suite build"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "turbo": "^2.0.6",
    "next": "15.4.7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "supports-color"
      ]
    }
  }
}